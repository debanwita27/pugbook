{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "plugin:node/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"indent": "off",
		"linebreak-style": "off",
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"react/prop-types": "off",
		"react/react-in-jsx-scope": "off",
		"no-console": "off",
		"node/no-unsupported-features/es-syntax": "off",
		"node/no-missing-import": "off"
	}
}
